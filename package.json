{
  "name": "cube-game",
  "version": "1.0.0",
  "description": "Webpack based JS web game",
  "main": "src/js/index.js",
  "scripts": {
    "webpack-dev-server": "webpack serve --config webpack.dev.js",
    "build:prod": "rm -rf public && webpack --config webpack.prod.js",
    "build:dev": "rm -rf public && webpack --config webpack.dev.js --watch",
    "start": "nodemon server.js",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuggsrocks/cube-game.git"
  },
