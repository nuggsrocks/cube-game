  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuggsrocks/cube-game/issues"
  },
  "homepage": "https://github.com/nuggsrocks/cube-game#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "file-loader": "^6.2.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "standard": "^16.0.4",
    "style-loader": "^3.3.1",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongodb": "^4.2.0"
  }
